<form action="/landmarks/<%=@landmark.id%>" method="post">
<input id="hidden" type="hidden" name="_method" value="patch">

  <label> Name: </label>
  <input type="text" name="landmark[name]" value= "<%=@landmark.name %>" >
  <br><br>
  <label> Year Completed: </label>
  <input type="text" name="landmark[year_completed]" value= "<%=@landmark.year_completed %>" >
  <br><br>

  <input type="submit" value="Update Landmark">
</form>
