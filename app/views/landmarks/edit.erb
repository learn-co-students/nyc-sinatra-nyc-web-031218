<form action="/landmarks/<%= @landmark.id %>" method="post">
  <input type="hidden" id="hidden" name="_method" value="patch">
  Name: <input type="text" id="name" name="landmark[name]" value="<%= @landmark.name %>">
  Year Completed: <input id="year_completed" type="text" value="<%= @landmark.year_completed %>" name="landmark[year_completed]">
  <input type="submit" name="Edit Landmark" value="Edit Landmark">
</form>
